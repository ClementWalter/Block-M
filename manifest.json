{
  "manifest_version": 2,
  "name": "Block M: the Black M blocker",
  "version": "0.1",
  "description": "This add prevents your computer from being irremediably corrupted by playing Black M",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.1.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions":[
    "tabs"
  ]
}