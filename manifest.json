{
  "manifest_version": 2,
  "name": "Block M: the Black M blocker",
  "version": "0.5",
  "description": "This add prevents your computer from being irremediably corrupted by playing Black M",
  "background": {
    "scripts": ["toFirstUpper.js", "background.js"]
  },
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "icons": { "16": "icon16.png",
          "128": "icon128.png" },
  "permissions":[
    "tabs",
    "tabCapture",
    "storage"
  ],
  "options_page": "options.html"
}